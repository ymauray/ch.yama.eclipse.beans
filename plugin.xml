		<cleanUpConfigurationUI
			class="ch.idinfo.eclipse.beans.propertiesupdate.PropertiesTabPage"
			name="Update properties"
			cleanUpKind="saveAction">
		</cleanUpConfigurationUI>
   </extension>

</plugin>
